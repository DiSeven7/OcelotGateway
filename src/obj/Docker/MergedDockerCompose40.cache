name: dockercompose13739804018929002369
services:
  ocelotapigw:
    build:
      context: C:\Users\David\Desktop\Proyectos .NET\OcelotGateway\src
      dockerfile: OcelotAPIGw/Dockerfile
    container_name: OcelotGateway
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: ocelotapigw
    networks:
      api: null
    ports:
      - mode: ingress
        target: 80
        published: "14000"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\David\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\David\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  redis:
    command:
      - /entrypoint.sh
      - ./redis-stack.conf
    container_name: RedisCache
    healthcheck:
      test:
        - CMD
        - redis-cli
        - --raw
        - incr
        - ping
    image: redis:latest
    networks:
      api: null
    ports:
      - mode: ingress
        target: 6379
        published: "14001"
        protocol: tcp
    volumes:
      - type: volume
        source: redis-data
        target: /data
        volume: {}
  usersapi:
    build:
      context: C:\Users\David\Desktop\Proyectos .NET\OcelotGateway\src
      dockerfile: UsersAPI/Dockerfile
    container_name: UsersAPI
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: usersapi
    networks:
      api: null
    ports:
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\David\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\David\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  usersdb:
    container_name: UsersDb
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_SA_PASSWORD: Users25!
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      api: null
    ports:
      - mode: ingress
        target: 1433
        published: "14002"
        protocol: tcp
    restart: always
networks:
  api:
    name: dockercompose13739804018929002369_api
    driver: bridge
volumes:
  redis-data:
    name: dockercompose13739804018929002369_redis-data